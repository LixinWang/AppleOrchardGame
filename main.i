void hideSprites()
{
    for(int i = 0; i < 128; i++) {
     shadowOAM[i].attr0 = (2 << 8);
    }
}


void updateOAM()
{

 if(basket.size == 0) {
  shadowOAM[0].attr0 = (0xFF & basket.row) | (0 << 13) | (0 << 14);
  shadowOAM[0].attr1 = (0x1FF & basket.col) | (2 << 14);
  shadowOAM[0].attr2 = ((5)*32+(0)) | ((0)<<12);
 } else if (basket.size == 1) {
  shadowOAM[0].attr0 = (0xFF & basket.row) | (0 << 13) | (0 << 14);
  shadowOAM[0].attr1 = (0x1FF & basket.col) | (2 << 14);
  shadowOAM[0].attr2 = ((5)*32+(4)) | ((0)<<12);
 } else {
  shadowOAM[0].attr0 = (0xFF & basket.row) | (0 << 13) | (1 << 14);
  shadowOAM[0].attr1 = (0x1FF & basket.col) | (3 << 14);
  shadowOAM[0].attr2 = ((5)*32+(8)) | ((0)<<12);
 }


 for(int i = 0; i < appleCount; i++) {
  if(apples[i].active) {
   if(apples[i].type == 1) {
    if (time%5 == 0) {
     shadowOAM[1+i].attr0 = (0xFF & apples[i].row) | (0 << 13) | (0 << 14);
     shadowOAM[1+i].attr1 = (0x1FF & apples[i].col) | (2 << 14);
     shadowOAM[1+i].attr2 = ((0)*32+(16)) | ((apples[i].type + 1)<<12);
    } else {
     shadowOAM[1+i].attr0 = (0xFF & apples[i].row) | (0 << 13) | (0 << 14);
     shadowOAM[1+i].attr1 = (0x1FF & apples[i].col) | (2 << 14);
     shadowOAM[1+i].attr2 = ((0)*32+(0 + apples[i].type *4)) | ((apples[i].type + 1)<<12);
    }
   } else if(apples[i].type == 3) {
    if (time%5 == 0) {
     shadowOAM[1+i].attr0 = (0xFF & apples[i].row) | (0 << 13) | (0 << 14);
     shadowOAM[1+i].attr1 = (0x1FF & apples[i].col) | (2 << 14);
     shadowOAM[1+i].attr2 = ((0)*32+(20)) | ((apples[i].type + 1)<<12);
    } else {
     shadowOAM[1+i].attr0 = (0xFF & apples[i].row) | (0 << 13) | (0 << 14);
     shadowOAM[1+i].attr1 = (0x1FF & apples[i].col) | (2 << 14);
     shadowOAM[1+i].attr2 = ((0)*32+(0 + apples[i].type *4)) | ((apples[i].type + 1)<<12);
    }
   } else {
    shadowOAM[1+i].attr0 = (0xFF & apples[i].row) | (0 << 13) | (0 << 14);
    shadowOAM[1+i].attr1 = (0x1FF & apples[i].col) | (2 << 14);
    shadowOAM[1+i].attr2 = ((0)*32+(0 + apples[i].type *4)) | ((apples[i].type + 1)<<12);
   }
  } else {
   shadowOAM[1+i].attr0 = (2 << 8);
  }
 }

 displayScore();
 drawLives();
 drawWarning();
 drawPauseText();
 drawNumApples();


    DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 128*4);
}






