 {
  playSoundA(gameSong, 841536, 11025, 1);
  initGame();
  goToGame();
 }

 if((!(~(oldButtons)&((1<<2))) && (~buttons & ((1<<2)))))
 {
  playSoundA(instructSong, 555552, 11025, 1);
  goToInstruct();
 }
}


void instructions() {

 if((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3)))))
 {
  if(oldState == 0) {
   initGame();
  }
  playSoundA(gameSong, 841536, 11025, 1);
  goToGame();
 }

 if((!(~(oldButtons)&((1<<2))) && (~buttons & ((1<<2)))))
 {
  playSoundA(menuSong,1289088,11025, 1);
  goToMenu();
 }
}


void game() {


 if(!(basket.lives == 0)) {
  hideSprites();
  update();
  waitForVblank();
  updateOAM();
 } else {


  goToEnd();
    }


 if((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3)))))
 {
  pauseSound();
  goToPause();
 }


 if((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))))
 {
  cheatCounter++;
  if(cheatCounter == 4) {
   playSoundB(gainLifeSFX,2940,11025, 0);
   basket.lives += 1;
  }
 }


 *(volatile unsigned short *)0x04000010 = hOff;
 *(volatile unsigned short *)0x04000014 = hOff/2;
}


void pause()
{

    if((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
     unpauseSound();
     goToGame();
    }


    if((!(~(oldButtons)&((1<<2))) && (~buttons & ((1<<2))))) {
     playSoundA(instructSong, 555552, 11025, 1);
     goToInstruct();
    }

    if((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0))))) {
     playSoundA(menuSong,1289088,11025, 1);
     goToMenu();
    }
}


void end()
{

    if((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
     playSoundA(menuSong,1289088,11025, 1);
     goToMenu();
    }
}
